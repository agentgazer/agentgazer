  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs');const p=require('path');const src=p.resolve(__dirname,'../../apps/dashboard-local/dist');const dst=p.resolve(__dirname,'dist/dashboard');if(fs.existsSync(src)){fs.cpSync(src,dst,{recursive:true});console.log('Copied dashboard to dist/dashboard')}else{console.warn('Warning: dashboard-local/dist not found')}\"",
    "dev": "tsc --watch",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
