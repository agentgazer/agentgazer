    "docs:build": "npm run build --workspace=docs",
    "docs:preview": "npm run preview --workspace=docs"
  },
  "packageManager": "npm@10.9.2",
  "devDependencies": {
    "turbo": "^2"
  },
  "overrides": {
    "esbuild": ">=0.25.0"
  },
  "version": "0.5.2"
}
